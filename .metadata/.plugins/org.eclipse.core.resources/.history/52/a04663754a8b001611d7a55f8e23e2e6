package a4novice;

public abstract class AnyPicture {
	private int width;
	private int height;
	private Picture source;
	private int xOffset;
	private int yOffset;
	private Pixel[][] arrPixels;
	
	
	public abstract SubPicture extract(int xOffset, int yOffset, int width, int height) {
		SubPicture subPic = new SubPictureImpl(source,xOffset,yOffset,width,height);
		return subPic;
	}
	
}
