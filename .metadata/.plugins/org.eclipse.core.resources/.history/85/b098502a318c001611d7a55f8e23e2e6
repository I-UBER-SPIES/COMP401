package a4jedi;

public class GammaCorrect implements PixelTransformation {
	private double gamma;
	public GammaCorrect(double gamma){
		this.gamma = gamma;
	}

	@Override
	public Pixel Transform(Pixel p) {
		
		return Pixel g = new ColorPixel(Math.pow(p.getRed(), 1/gamma),Math.pow(p.getGreen(), 1/gamma),Math.pow(p.getBlue(), 1/gamma));
		
	}

}
